{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.23",
    "axios": "^0.25.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-notifications-component": "^4.0.0",
    "react-redux": "^8.0.1",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.3",
    "react-select-2": "^2.0.11",
    "styled-components": "^5.3.3",
    "tsc": "^2.0.4",
    "typescript": "^4.6.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom"
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.17",
    "customize-cra": "^1.0.0",
    "i": "^0.3.7",
    "npm": "^8.5.4",
    "ou": "^0.0.2",
    "prettier": "^2.5.1",
    "react-app-rewired": "^2.2.1",
    "types": "^0.1.1"
  },
  "format": "prettier --write \"src/**/*.{js,jsx}\"",
  "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
}
